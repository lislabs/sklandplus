{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "integer"
    },
    "message": {
      "type": "string"
    },
    "data": {
      "$ref": "#/definitions/Data"
    }
  },
  "required": ["code", "data", "message"],
  "title": "GetGamePlayerInfoResponse",
  "definitions": {
    "Data": {
      "type": "object",
      "properties": {
        "currentTs": {
          "type": "integer",
          "description": "Timestamp in seconds",
          "examples": [1554105600]
        },
        "showConfig": {
          "$ref": "#/definitions/ShowConfig"
        },
        "status": {
          "$ref": "#/definitions/Status"
        },
        "medal": {
          "$ref": "#/definitions/Medal"
        },
        "assistChars": {
          "type": "array",
          "description": "User's assistant operators/support unit (助战干员) configuration.",
          "items": {
            "$ref": "#/definitions/AssistChar"
          }
        },
        "chars": {
          "type": "array",
          "description": "List of operators (干员) the user owns.",
          "items": {
            "$ref": "#/definitions/DataChar"
          }
        },
        "skins": {
          "type": "array",
          "description": "List of skins (干员服装) the user owns.",
          "items": {
            "$ref": "#/definitions/Skin"
          }
        },
        "building": {
          "$ref": "#/definitions/Building"
        },
        "recruit": {
          "type": "array",
          "description": "User's current recruit (公开招募) status.",
          "items": {
            "$ref": "#/definitions/Recruit"
          }
        },
        "campaign": {
          "$ref": "#/definitions/Campaign"
        },
        "tower": {
          "$ref": "#/definitions/Tower"
        },
        "rogue": {
          "$ref": "#/definitions/Rogue"
        },
        "routine": {
          "$ref": "#/definitions/Routine"
        },
        "activity": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Activity"
          }
        },
        "charInfoMap": {
          "$ref": "#/definitions/CharInfoMap"
        },
        "skinInfoMap": {
          "$ref": "#/definitions/SkinInfoMap"
        },
        "stageInfoMap": {
          "$ref": "#/definitions/StageInfoMap"
        },
        "activityInfoMap": {
          "$ref": "#/definitions/ActivityInfoMap"
        },
        "zoneInfoMap": {
          "$ref": "#/definitions/CampaignZoneInfoMap"
        },
        "towerInfoMap": {
          "$ref": "#/definitions/TowerInfoMap"
        },
        "rogueInfoMap": {
          "$ref": "#/definitions/RogueInfoMap"
        },
        "campaignInfoMap": {
          "$ref": "#/definitions/CampaignInfoMap"
        },
        "medalInfoMap": {
          "$ref": "#/definitions/MedalInfoMap"
        },
        "campaignZoneInfoMap": {
          "$ref": "#/definitions/CampaignZoneInfoMap"
        },
        "equipmentInfoMap": {
          "$ref": "#/definitions/EquipmentInfoMap"
        },
        "manufactureFormulaInfoMap": {
          "$ref": "#/definitions/ManufactureFormulaInfoMap"
        },
        "charAssets": {
          "type": "array",
          "items": {}
        },
        "skinAssets": {
          "type": "array",
          "items": {}
        }
      },
      "required": [
        "activity",
        "activityInfoMap",
        "assistChars",
        "building",
        "campaign",
        "campaignInfoMap",
        "campaignZoneInfoMap",
        "charAssets",
        "charInfoMap",
        "chars",
        "currentTs",
        "equipmentInfoMap",
        "manufactureFormulaInfoMap",
        "medal",
        "medalInfoMap",
        "recruit",
        "rogue",
        "rogueInfoMap",
        "routine",
        "showConfig",
        "skinAssets",
        "skinInfoMap",
        "skins",
        "stageInfoMap",
        "status",
        "tower",
        "towerInfoMap",
        "zoneInfoMap"
      ],
      "title": "Data"
    },
    "Activity": {
      "type": "object",
      "properties": {
        "actId": {
          "type": "string"
        },
        "actReplicaId": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "zones": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Zone"
          }
        }
      },
      "required": ["actId", "actReplicaId", "type", "zones"],
      "title": "Activity"
    },
    "Zone": {
      "type": "object",
      "properties": {
        "zoneId": {
          "type": "string"
        },
        "zoneReplicaId": {
          "type": "string"
        },
        "clearedStage": {
          "type": "integer"
        },
        "totalStage": {
          "type": "integer"
        },
        "stageStatus": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/StageStatus"
          }
        }
      },
      "required": [
        "clearedStage",
        "stageStatus",
        "totalStage",
        "zoneId",
        "zoneReplicaId"
      ],
      "title": "Zone"
    },
    "StageStatus": {
      "type": "object",
      "properties": {
        "stageId": {
          "type": "string"
        },
        "completed": {
          "type": "boolean"
        }
      },
      "required": ["completed", "stageId"],
      "title": "StageStatus"
    },
    "ActivityInfoMap": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "startTime": {
          "type": "integer"
        },
        "endTime": {
          "type": "integer"
        },
        "rewardEndTime": {
          "type": "integer"
        },
        "isReplicate": {
          "type": "boolean"
        },
        "type": {
          "$ref": "#/definitions/ActivityInfoMapType"
        }
      },
      "required": [
        "endTime",
        "id",
        "isReplicate",
        "name",
        "rewardEndTime",
        "startTime",
        "type"
      ],
      "title": "ActivityInfoMap"
    },
    "AssistChar": {
      "type": "object",
      "properties": {
        "charId": {
          "type": "string",
          "description": "In-game Operator (干员) ID.",
          "examples": ["char_1028_texas2"]
        },
        "skinId": {
          "type": "string",
          "description": "In-game Skin (干员服装) ID.",
          "examples": ["char_1028_texas2#2"]
        },
        "level": {
          "type": "integer",
          "description": "Operator's level (干员等级). Maximum value depends on the rarity of the operator.",
          "minimum": 1,
          "maximum": 90
        },
        "evolvePhase": {
          "type": "integer",
          "description": "Operator's current evolution phase (精英化等级). Maximum value depends on the rarity of the operator.",
          "enum": [0, 1, 2],
          "minimum": 0,
          "maximum": 2
        },
        "potentialRank": {
          "type": "integer",
          "description": "Operator's potential rank (潜能等级). Maximum value is 6. Initial value is 1 (right after the user acquired the operator).",
          "enum": [1, 2, 3, 4, 5, 6],
          "minimum": 1,
          "maximum": 6
        },
        "skillId": {
          "type": "string",
          "description": "Operator's current skill (技能) ID that is configured as the assistance skill (助战技能).",
          "examples": ["skchr_texas2_3"]
        },
        "mainSkillLvl": {
          "type": "integer",
          "description": "Operator's current skill (技能) level (技能等级). This level does not include the \"specialize\" level (专精等级).",
          "minimum": 1,
          "maximum": 7
        },
        "specializeLevel": {
          "type": "integer",
          "description": "Operator's current skill (技能) specialize level (专精等级).",
          "minimum": 0,
          "maximum": 3
        },
        "equip": {
          "description": "Equipment (模组) the operator is currently equipped with. A `null` value means the operator is not equipped with any equipment.",
          "anyOf": [
            {
              "$ref": "#/definitions/Equip"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "required": [
        "charId",
        "equip",
        "evolvePhase",
        "level",
        "mainSkillLvl",
        "potentialRank",
        "skillId",
        "skinId",
        "specializeLevel"
      ],
      "title": "AssistChar"
    },
    "Equip": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "In-game Equipment (模组) ID."
        },
        "level": {
          "type": "integer",
          "description": "Equipment's level (模组等级).",
          "minimum": 1,
          "maximum": 3
        }
      },
      "required": ["id", "level"],
      "title": "Equip",
      "description": "Equipment (模组) of an operator."
    },
    "Building": {
      "type": "object",
      "description": "User's base (基建) status.",
      "properties": {
        "tiredChars": {
          "type": "array",
          "description": "List of operators (干员) that are currently tired (注意力涣散). TODO: Find out the item types in this array.",
          "items": {}
        },
        "powers": {
          "type": "array",
          "description": "Status of the Power Stations (发电站).",
          "items": {
            "$ref": "#/definitions/Control"
          }
        },
        "manufactures": {
          "type": "array",
          "description": "Status of the Manufacture Stations (制造站).",
          "items": {
            "$ref": "#/definitions/Manufacture"
          }
        },
        "tradings": {
          "type": "array",
          "description": "Status of the Trading Stations (贸易站).",
          "items": {
            "$ref": "#/definitions/Trading"
          }
        },
        "dormitories": {
          "type": "array",
          "description": "Status of the Dormitories (宿舍).",
          "items": {
            "$ref": "#/definitions/Control"
          }
        },
        "meeting": {
          "$ref": "#/definitions/Meeting",
          "description": "Status of the Meeting Room (会客室)."
        },
        "hire": {
          "$ref": "#/definitions/Hire"
        },
        "training": {
          "$ref": "#/definitions/Training"
        },
        "labor": {
          "$ref": "#/definitions/Labor"
        },
        "furniture": {
          "$ref": "#/definitions/Furniture"
        },
        "elevators": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Control"
          }
        },
        "corridors": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Control"
          }
        },
        "control": {
          "$ref": "#/definitions/Control"
        }
      },
      "required": [
        "control",
        "corridors",
        "dormitories",
        "elevators",
        "furniture",
        "hire",
        "labor",
        "manufactures",
        "meeting",
        "powers",
        "tiredChars",
        "tradings",
        "training"
      ],
      "title": "Building"
    },
    "ControlChar": {
      "type": "object",
      "properties": {
        "charId": {
          "type": "string",
          "description": "In-game Operator (干员) ID.",
          "examples": ["char_1028_texas2"]
        },
        "ap": {
          "type": "integer",
          "description": "Operator's current AP (注意力).",
          "minimum": 0,
          "maximum": 8640000
        },
        "lastApAddTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the operator's AP (注意力) was last added."
        },
        "index": {
          "type": "integer",
          "description": "Operator's index (位置). 0 means the operator is located at the left-most operator slot within the game.",
          "minimum": 0,
          "maximum": 4
        },
        "bubble": {
          "$ref": "#/definitions/Bubble"
        },
        "workTime": {
          "type": "integer",
          "description": "Operator's current work time (工作时间) in minutes (分钟)."
        }
      },
      "required": [
        "ap",
        "bubble",
        "charId",
        "index",
        "lastApAddTime",
        "workTime"
      ],
      "title": "ControlChar"
    },
    "Bubble": {
      "type": "object",
      "properties": {
        "normal": {
          "$ref": "#/definitions/Assist"
        },
        "assist": {
          "$ref": "#/definitions/Assist"
        }
      },
      "required": ["assist", "normal"],
      "description": "Operator (干员) bubble (信赖收取气泡) status.",
      "title": "Bubble"
    },
    "Assist": {
      "type": "object",
      "properties": {
        "add": {
          "type": "integer"
        },
        "ts": {
          "type": "integer"
        }
      },
      "required": ["add", "ts"],
      "title": "Assist"
    },
    "Control": {
      "type": "object",
      "properties": {
        "slotId": {
          "type": "string",
          "description": "In-game building slot (基建设施建造位) ID.",
          "examples": ["slot_26"]
        },
        "slotState": {
          "type": "integer"
        },
        "level": {
          "type": "integer",
          "description": "Building's level (基建设施等级). Regular buildings can be upgraded up to level 3. Dormitories (宿舍) can be upgraded up to level 5.",
          "minimum": 1,
          "maximum": 5
        },
        "chars": {
          "type": "array",
          "description": "List of operators (干员) that are currently located (进驻) in the building.",
          "items": {
            "$ref": "#/definitions/ControlChar"
          }
        },
        "comfort": {
          "type": "integer",
          "description": "Building's comfort (舒适度). Only exists in dormitories (宿舍).",
          "minimum": 0,
          "maximum": 5000
        }
      },
      "required": ["level", "slotId"],
      "title": "Control"
    },
    "Furniture": {
      "type": "object",
      "properties": {
        "total": {
          "type": "integer"
        }
      },
      "required": ["total"],
      "title": "Furniture"
    },
    "Hire": {
      "type": "object",
      "properties": {
        "slotId": {
          "type": "string"
        },
        "level": {
          "type": "integer"
        },
        "chars": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ControlChar"
          }
        },
        "state": {
          "type": "integer"
        },
        "refreshCount": {
          "type": "integer"
        },
        "completeWorkTime": {
          "type": "integer"
        },
        "slotState": {
          "type": "integer"
        }
      },
      "required": [
        "chars",
        "completeWorkTime",
        "level",
        "refreshCount",
        "slotId",
        "slotState",
        "state"
      ],
      "title": "Hire"
    },
    "Labor": {
      "type": "object",
      "properties": {
        "maxValue": {
          "type": "integer"
        },
        "value": {
          "type": "integer"
        },
        "lastUpdateTime": {
          "type": "integer"
        },
        "remainSecs": {
          "type": "integer"
        }
      },
      "required": ["lastUpdateTime", "maxValue", "remainSecs", "value"],
      "title": "Labor"
    },
    "Manufacture": {
      "type": "object",
      "properties": {
        "slotId": {
          "type": "string",
          "description": "In-game building slot (基建设施建造位) ID.",
          "examples": ["slot_1"]
        },
        "level": {
          "type": "integer",
          "description": "Building's level (基建设施等级).",
          "minimum": 1,
          "maximum": 3
        },
        "chars": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ControlChar"
          }
        },
        "completeWorkTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the manufacture (制造站) will complete its current manufacture.",
          "examples": [1554105600]
        },
        "lastUpdateTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the manufacture (制造站) was last updated. TODO: Find out what this timestamp is used for.",
          "examples": [1554105600]
        },
        "formulaId": {
          "type": "string",
          "format": "integer",
          "description": "In-game manufacture formula (制造配方) ID.",
          "examples": ["3"]
        },
        "capacity": {
          "type": "integer",
          "description": "Manufacture's capacity (容量)."
        },
        "weight": {
          "type": "integer",
          "description": "Manufacture's weight (负载)? TODO: Find out what this value is used for.",
          "examples": [0]
        },
        "complete": {
          "type": "integer",
          "description": "Manufacture's complete (完成) status? TODO: Find out what this value is used for.",
          "examples": [0]
        },
        "remain": {
          "type": "integer",
          "description": "Manufacture's remain (剩余) status? TODO: Find out what this value is used for.",
          "examples": [99]
        },
        "speed": {
          "type": "number",
          "description": "Manufacture's speed (速度) multiplier.",
          "minimum": 0,
          "maximum": 2
        }
      },
      "required": [
        "capacity",
        "chars",
        "complete",
        "completeWorkTime",
        "formulaId",
        "lastUpdateTime",
        "level",
        "remain",
        "slotId",
        "speed",
        "weight"
      ],
      "title": "Manufacture"
    },
    "Meeting": {
      "type": "object",
      "properties": {
        "slotId": {
          "type": "string",
          "description": "In-game building slot (基建设施建造位) ID.",
          "examples": ["slot_1"]
        },
        "level": {
          "type": "integer",
          "description": "Building's level (基建设施等级).",
          "minimum": 1,
          "maximum": 3
        },
        "chars": {
          "type": "array",
          "description": "List of operators (干员) that are currently located (进驻) in the building.",
          "items": {
            "$ref": "#/definitions/ControlChar"
          }
        },
        "clue": {
          "$ref": "#/definitions/Clue"
        },
        "lastUpdateTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the meeting (会客室) was last updated. TODO: Find out what this timestamp is used for.",
          "examples": [1554105600]
        },
        "completeWorkTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the operators (干员) in the meeting (会客室) will become tired (注意力涣散).",
          "examples": [1554105600]
        }
      },
      "required": [
        "chars",
        "clue",
        "completeWorkTime",
        "lastUpdateTime",
        "level",
        "slotId"
      ],
      "title": "Meeting"
    },
    "Clue": {
      "type": "object",
      "description": "Clue (线索) status.",
      "properties": {
        "own": {
          "type": "integer",
          "description": "Amount of clues (线索) the user currently owns."
        },
        "received": {
          "type": "integer",
          "description": "Amount of clues (线索) the user has received."
        },
        "dailyReward": {
          "type": "boolean",
          "description": "Whether the user has received the daily clue (线索) reward."
        },
        "needReceive": {
          "type": "integer",
          "description": "Amount of clues (线索) that is available for the user to be received."
        },
        "board": {
          "type": "array",
          "description": "List of clues (线索) the user currently has on the board (线索槽). TODO: add more types.",
          "items": {
            "type": "string",
            "enum": ["RHINE", "PENGUIN", "BLACKSTEEL", "RHODES", "GLASGOW"]
          }
        },
        "sharing": {
          "type": "boolean",
          "description": "Whether the user is currently in sharing (线索交流开启中) status.",
          "examples": [false]
        },
        "shareCompleteTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the sharing (线索交流) will complete.",
          "examples": [1554105600]
        }
      },
      "required": [
        "board",
        "dailyReward",
        "needReceive",
        "own",
        "received",
        "shareCompleteTime",
        "sharing"
      ],
      "title": "Clue"
    },
    "Trading": {
      "type": "object",
      "properties": {
        "slotId": {
          "type": "string",
          "description": "In-game building slot (基建设施建造位) ID.",
          "examples": ["slot_1"]
        },
        "level": {
          "type": "integer",
          "description": "Building's level (基建设施等级).",
          "minimum": 1,
          "maximum": 3
        },
        "chars": {
          "type": "array",
          "description": "List of operators (干员) that are currently located (进驻) in the building.",
          "items": {
            "$ref": "#/definitions/ControlChar"
          }
        },
        "completeWorkTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the trading (贸易站) will complete its current trade.",
          "examples": [1554105600]
        },
        "lastUpdateTime": {
          "type": "integer",
          "description": "Timestamp (in seconds) when the trading (贸易站) was last updated. TODO: Find out what this timestamp is used for.",
          "examples": [1554105600]
        },
        "strategy": {
          "$ref": "#/definitions/Strategy"
        },
        "stock": {
          "type": "array",
          "description": "List of stocks (库存) the trading (贸易站) currently has.",
          "items": {
            "$ref": "#/definitions/Stock"
          }
        },
        "stockLimit": {
          "type": "integer",
          "description": "Trading's stock limit (库存上限)."
        }
      },
      "required": [
        "chars",
        "completeWorkTime",
        "lastUpdateTime",
        "level",
        "slotId",
        "stock",
        "stockLimit",
        "strategy"
      ],
      "description": "Trading (贸易站) building.",
      "title": "Trading"
    },
    "Stock": {
      "type": "object",
      "properties": {
        "instId": {
          "type": "integer",
          "format": "integer"
        },
        "type": {
          "$ref": "#/definitions/Strategy"
        },
        "delivery": {
          "type": "array",
          "description": "List of items needed to deliver (交付) in order to get the gain (收益). These items will be consumed when the delivery is complete.",
          "items": {
            "$ref": "#/definitions/StockItem"
          }
        },
        "gain": {
          "$ref": "#/definitions/StockItem",
          "description": "Gain (收益) the user will get when the delivery is complete."
        },
        "isViolated": {
          "type": "boolean",
          "description": "TODO: Find out what this value is used for."
        }
      },
      "required": ["delivery", "gain", "instId", "isViolated", "type"],
      "title": "Stock"
    },
    "StockItem": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "integer",
          "description": "In-game item (物品) ID."
        },
        "count": {
          "type": "integer",
          "description": "Amount of the item (物品)."
        },
        "type": {
          "$ref": "#/definitions/ItemType"
        }
      },
      "required": ["count", "id", "type"],
      "title": "StockItem"
    },
